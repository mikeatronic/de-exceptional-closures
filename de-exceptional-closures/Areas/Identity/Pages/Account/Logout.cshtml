@page
@model LogoutModel
@{
    ViewData["Title"] = "Log out";
}

<main class="govuk-main-wrapper" id="main-content" role="main">
    <div class="govuk-grid-row">
        <div class="govuk-width-container">
            <div class="govuk-grid-row">
                <div class="govuk-grid-column-full">

                    <h1 class="govuk-heading-l">You have successfully logged out of the application.</h1>

                    <a asp-area="" asp-controller="Home" asp-action="Index" role="button" draggable="false" class="govuk-button" data-module="govuk-button">Log in</a>

                </div>
            </div>
        </div>
        </div>
    </main>